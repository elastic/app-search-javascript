{
  "name": "swiftype-app-search-javascript",
  "version": "2.4.0",
  "description": "Javascript client for the Swiftype App Search Api",
  "browser": "dist/swiftype_app_search.umd.js",
  "main": "dist/swiftype_app_search.umd.js",
  "module": "dist/swiftype_app_search.es.js",
  "scripts": {
    "test": "BABEL_ENV=test jest",
    "format": "prettier --write \"**/*.js\"",
    "build": "BABEL_ENV=dev rollup -c",
    "watch": "BABEL_ENV=dev rollup -c --watch",
    "predev": "npm run watch &",
    "dev": "http-server dist",
    "prepare": "npm run build",
    "precommit": "lint-staged"
  },
  "lint-staged": {
    "*.js": [
      "eslint --fix",
      "git add"
    ],
    "*.{json,css,md}": [
      "prettier --write",
      "git add"
    ]
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/swiftype/swiftype-app-search-javascript.git"
  },
  "author": "Swiftype",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/swiftype/swiftype-app-search-javascript/issues"
  },
  "homepage": "https://github.com/swiftype/swiftype-app-search-javascript",
  "dependencies": {
    "object-hash": "^1.3.0"
  },
  "devDependencies": {
    "babel-cli": "^6.24.1",
    "babel-core": "^6.26.3",
    "babel-jest": "^22.4.3",
    "babel-plugin-external-helpers": "^6.22.0",
    "babel-plugin-transform-object-rest-spread": "^6.26.0",
    "babel-preset-env": "^1.6.1",
    "babel-runtime": "^6.9.2",
    "eslint": "^5.1.0",
    "eslint-config-prettier": "^2.9.0",
    "eslint-plugin-prettier": "^2.6.2",
    "http-server": "^0.11.1",
    "husky": "^0.14.3",
    "jest": "^22.4.3",
    "lint-staged": "^7.2.0",
    "node-fetch": "^2.1.2",
    "prettier": "1.13.7",
    "regenerator-runtime": "^0.11.1",
    "replay": "^2.3.0",
    "rollup": "^0.62.0",
    "rollup-plugin-babel": "^3.0.7",
    "rollup-plugin-json": "^3.0.0",
    "rollup-plugin-uglify": "^4.0.0"
  }
}
